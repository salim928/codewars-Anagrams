    "def anagrams(word, words):\n",
    "    #arranging x in word in ascending order\n",
    "    x = sorted(word)\n",
    "    #return w as an array for w in words if x is equal to sorted w else none\n",
    "    return [w for w in words if x ==sorted(w)]"
   ]
